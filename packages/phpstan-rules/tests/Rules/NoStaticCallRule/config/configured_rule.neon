services:
    -
        class: Symplify\PHPStanRules\Rules\ValueObject\NoStaticCallRule
        tags: [phpstan.rules.rule]
        arguments:
            allowedStaticCallClasses:
                - Symplify\PHPStanRules\Tests\Rules\NoStaticCallRule\Source\AllowedMethods

    - Symplify\PackageBuilder\Matcher\ValueObject\ArrayStringAndFnMatcher
