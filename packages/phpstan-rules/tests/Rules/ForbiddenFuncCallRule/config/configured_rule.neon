services:
    - Symplify\PackageBuilder\Matcher\ValueObject\ArrayStringAndFnMatcher
    -
        class: Symplify\PHPStanRules\Rules\ValueObject\ForbiddenFuncCallRule
        tags: [phpstan.rules.rule]
        arguments:
            forbiddenFunctions:
                - 'extract'
                - 'dump'
                - 'curl_*'
